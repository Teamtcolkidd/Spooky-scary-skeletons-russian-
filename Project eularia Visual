local mainFrame = Instance.new("Frame", screenGui)
mainFrame.Size = UDim2.new(0, 470, 0, 250)
mainFrame.Position = UDim2.new(0.25, 0, 0.25, 0)
mainFrame.BackgroundColor3 = black
mainFrame.BorderColor3 = neonGreen
mainFrame.BorderSizePixel = 4
mainFrame.Active = true
mainFrame.Draggable = true

local topBar = Instance.new("Frame", mainFrame)
topBar.Size = UDim2.new(1, 0, 0, 30)
topBar.BackgroundColor3 = black
topBar.BorderColor3 = neonGreen
topBar.BorderSizePixel = 2

local function createTab(name, posX, width, isButton)
	local tab
	if isButton then
		tab = Instance.new("TextButton", topBar)
	else
		tab = Instance.new("TextLabel", topBar)
	end
	tab.Size = UDim2.new(0, width, 1, 0)
	tab.Position = UDim2.new(0, posX, 0, 0)
	tab.BackgroundColor3 = black
	tab.BorderColor3 = neonGreen
	tab.BorderSizePixel = 2
	tab.Font = Enum.Font.SourceSansBold
	tab.TextColor3 = white
	tab.TextScaled = true
	tab.Text = name
	if isButton then tab.AutoButtonColor = false end
	return tab
end

