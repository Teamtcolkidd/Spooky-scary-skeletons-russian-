addTextBox2.Position = UDim2.new(0.05, 0, 0, 80)
addTextBox2.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
addTextBox2.TextColor3 = white
addTextBox2.PlaceholderText = "URL"
addTextBox2.Font = Enum.Font.SourceSans
addTextBox2.TextSize = 16
addTextBox2.ClearTextOnFocus = false

-- Bot√≥n Add Script
local addScriptBtn = Instance.new("TextButton", addFrame)
addScriptBtn.Size = UDim2.new(0.9, 0, 0, 35)
addScriptBtn.Position = UDim2.new(0.05, 0, 0, 120)
addScriptBtn.BackgroundColor3 = black
addScriptBtn.BorderColor3 = neonGreen
addScriptBtn.BorderSizePixel = 3
addScriptBtn.TextColor3 = white
addScriptBtn.Font = Enum.Font.SourceSansBold
addScriptBtn.TextScaled = true
addScriptBtn.Text = "Add Script"

-- Funci√≥n al presionar Add Script
addScriptBtn.MouseButton1Click:Connect(function()
	local newScript = {
		Text = addTextBox1.Text,
		URL = addTextBox2.Text
	}
	if newScript.Text ~= "" and newScript.URL ~= "" then
		-- Insertar en la tabla allScripts para que quede en la lista
		table.insert(allScripts, 1, newScript)

		-- Crear bot√≥n en scriptsFrame
		local btn = Instance.new("TextButton", scriptsFrame)
		btn.Size = UDim2.new(1, -10, 0, 30)
		btn.Position = UDim2.new(0, 5, 0, 35) -- arriba despu√©s de Add
		btn.BackgroundColor3 = black
		btn.BorderColor3 = neonGreen
		btn.BorderSizePixel = 2
		btn.TextColor3 = white
		btn.Font = Enum.Font.SourceSansBold
		btn.TextScaled = true
		btn.Text = newScript.Text

		btn.MouseButton1Click:Connect(function()
			pcall(function()
				loadstring(game:HttpGet(newScript.URL))()
			end)
		end)

		-- Reposicionar dem√°s botones
		for i, child in ipairs(scriptsFrame:GetChildren()) do
			if child:IsA("TextButton") and child ~= addBtn and child ~= btn then
				child.Position = UDim2.new(0, 5, 0, child.Position.Y.Offset + 35)
			end
		end
	end

	-- Limpiar TextBoxes y cerrar ventana
	addTextBox1.Text = ""
	addTextBox2.Text = ""
	addFrame.Visible = false
end)

-- Abrir ventana Add
addBtn.MouseButton1Click:Connect(function()
	addFrame.Visible = true
end)

