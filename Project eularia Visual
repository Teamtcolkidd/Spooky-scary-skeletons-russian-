local scriptsFrame = Instance.new("ScrollingFrame", mainFrame)
scriptsFrame.Position = UDim2.new(0, 0, 0, 30)
scriptsFrame.Size = UDim2.new(0.75, 0, 1, -30)
scriptsFrame.BackgroundColor3 = black
scriptsFrame.BorderColor3 = neonGreen
scriptsFrame.BorderSizePixel = 2
scriptsFrame.CanvasSize = UDim2.new(0, 0, 0, 0) -- lo dejamos din√°mico
scriptsFrame.ScrollBarThickness = 6
scriptsFrame.ScrollingDirection = Enum.ScrollingDirection.Y
scriptsFrame.Visible = false

-- üîπ Funci√≥n para actualizar el tama√±o del scroll seg√∫n los botones
local function updateCanvasSize()
	local totalButtons = 0
	for _, child in ipairs(scriptsFrame:GetChildren()) do
		if child:IsA("TextButton") then
			totalButtons += 1
		end
	end
	scriptsFrame.CanvasSize = UDim2.new(0, 0, 0, totalButtons * 35)
end

-- üîπ Crear los botones iniciales desde allScripts
for i, scriptInfo in ipairs(allScripts) do
	local btn = Instance.new("TextButton", scriptsFrame)
	btn.Size = UDim2.new(1, -10, 0, 30)
	btn.Position = UDim2.new(0, 5, 0, (i - 1) * 35)
	btn.BackgroundColor3 = black
	btn.BorderColor3 = neonGreen
	btn.BorderSizePixel = 2
	btn.TextColor3 = white
	btn.Font = Enum.Font.SourceSansBold
	btn.TextScaled = true
	btn.Text = scriptInfo.Text
	btn.MouseButton1Click:Connect(function()
		loadstring(game:HttpGet(scriptInfo.URL))()
	end)

	updateCanvasSize() -- üî• actualiza el scroll cada vez que se a√±ade un bot√≥n
end
