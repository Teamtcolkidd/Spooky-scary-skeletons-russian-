setWelcomeVisible(true)
contentFrame.Visible = true
scriptsFrame.Visible = false
extraFrame.Visible = false     


-- Project Ligma Intro by TC0likesScripting (con frame blanco junto al texto)

local TweenService = game:GetService("TweenService")
local CoreGui = game:GetService("CoreGui")

-- GUI principal
local gui = Instance.new("ScreenGui")
gui.Name = "executor"
gui.Parent = CoreGui
gui.ResetOnSpawn = false

-- Imagen principal
local lel = Instance.new("ImageLabel")
lel.Size = UDim2.new(0, 100, 0, 100)
lel.Position = UDim2.new(-0.127, 0, 0.408, 0)
lel.BackgroundTransparency = 1
lel.Image = "rbxassetid://134963889843907" -- logo principal
lel.Parent = gui

-- Anillo giratorio
local Ring = Instance.new("ImageLabel")
Ring.Size = UDim2.new(1, 0, 1, 0)
Ring.BackgroundTransparency = 1
Ring.Image = "rbxassetid://134963889843907" -- anillo decorativo
Ring.Parent = lel

-- √ìrbita
local Orbit = Instance.new("ImageLabel")
Orbit.Size = UDim2.new(0, 21, 0, 21)
Orbit.Position = UDim2.new(0, 0, 0, 10)
Orbit.BackgroundTransparency = 1
Orbit.Image = "rbxassetid://" -- punto decorativo
Orbit.Parent = Ring

-- Frame blanco (aparece junto al texto)
local WhiteFrame = Instance.new("Frame")
WhiteFrame.Parent = lel
WhiteFrame.BackgroundColor3 = Color3.new(0,0,0) -- blanco
WhiteFrame.BorderColor3 = Color3.fromRGB(153,255,0)
WhiteFrame.BorderSizePixel = 1
WhiteFrame.Position = UDim2.new(1, 10, 0, 10)
WhiteFrame.Size = UDim2.new(0, 270, 0, 80)
WhiteFrame.BackgroundTransparency = 1 -- empieza invisible

-- Texto
local TextLabel_2 = Instance.new("TextLabel")
TextLabel_2.Parent = WhiteFrame
TextLabel_2.BackgroundTransparency = 1
TextLabel_2.Size = UDim2.new(1, 0, 1, 0)
TextLabel_2.Font = Enum.Font.SourceSansBold
TextLabel_2.Text = "Project eularia by TC0LKIDD | Run your Lua Scripts! Script similar to POLARIA. Status: Injected!"
TextLabel_2.TextColor3 = Color3.new(255,255,255)
TextLabel_2.TextScaled = true
TextLabel_2.TextTransparency = 1
TextLabel_2.TextWrapped = true

-- Animaciones
lel:TweenPosition(UDim2.new(0.458, 0, 0.408, 0), "Out", "Linear", 0.4, false)
task.wait(3)
lel:TweenPosition(UDim2.new(0, 9, 0, 190), "Out", "Sine", 0.7, false)

-- Mostrar frame y texto con efecto fade-in
TweenService:Create(WhiteFrame, TweenInfo.new(1, Enum.EasingStyle.Cubic, Enum.EasingDirection.Out), {
	BackgroundTransparency = 0
}):Play()

TweenService:Create(TextLabel_2, TweenInfo.new(1.2, Enum.EasingStyle.Cubic, Enum.EasingDirection.Out), {
	TextTransparency = 0
}):Play()

-- Rotaci√≥n infinita
task.spawn(function()
	while task.wait() do
		Ring.Rotation += 1
		Orbit.Rotation -= 1
	end
end)
