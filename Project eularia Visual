recommendedLabel.TextScaled = true
recommendedLabel.Text = "Recommended"

local chosen = {}
while #chosen < 13 do
    local pick = allScripts[math.random(1, #allScripts)]
    if pick.Text ~= "R6" and pick.Text ~= "No work" and not table.find(chosen, pick) then
        table.insert(chosen, pick)
    end
end

for i, scriptInfo in ipairs(chosen) do
	local btn = Instance.new("TextButton", recommendedFrame)
	btn.Size = UDim2.new(1, -10, 0, 30)
	btn.Position = UDim2.new(0, 5, 0, 35 * i)
	btn.BackgroundColor3 = black
	btn.BorderColor3 = neonGreen
	btn.BorderSizePixel = 2
	btn.TextColor3 = white
	btn.Font = Enum.Font.SourceSansBold
	btn.TextScaled = true
	btn.Text = scriptInfo.Text
	btn.MouseButton1Click:Connect(function()
		loadstring(game:HttpGet(scriptInfo.URL))()
	end)
end

-- Pesta√±as
local function setWelcomeVisible(visible)
	profileImage.Visible = visible
	nameLabel.Visible = visible
	userTag.Visible = visible
end

tab2.MouseButton1Click:Connect(function()
	setWelcomeVisible(true)
	contentFrame.Visible = true
	scriptsFrame.Visible = false
	extraFrame.Visible = false
end)

tab3.MouseButton1Click:Connect(function()
	setWelcomeVisible(false)
	contentFrame.Visible = false
	scriptsFrame.Visible = true
	extraFrame.Visible = false
end)

tab4.MouseButton1Click:Connect(function()
	setWelcomeVisible(false)
	contentFrame.Visible = false
	scriptsFrame.Visible = false
	extraFrame.Visible = true
end)

collapseBtn.MouseButton1Click:Connect(function()
	setWelcomeVisible(false)
	contentFrame.Visible = false
	scriptsFrame.Visible = false
	extraFrame.Visible = false
end)

squareBtn.MouseButton1Click:Connect(function()
	if contentFrame.Visible == false and scriptsFrame.Visible == false and extraFrame.Visible == false then
		tab2:Activate()
		contentFrame.Visible = true
	end
end)

closeBtn.MouseButton1Click:Connect(function()
	screenGui:Destroy()
end)

